@article{kleinheerenbrinkOptimizationAvianPerching2022,
  title = {Optimization of Avian Perching Manoeuvres},
  author = {KleinHeerenbrink, Marco and France, Lydia A. and Brighton, Caroline H. and Taylor, Graham K.},
  year = 2022,
  month = jul,
  journal = {Nature},
