@article{carruthersAutomaticAeroelasticDevices2007,
  title = {Automatic Aeroelastic Devices in the Wings of a Steppe Eagle {{Aquila}} Nipalensis},
  author = {Carruthers, Anna C. and Thomas, Adrian L. R. and Taylor, Graham K.},
