}

@article{nabawyRoleLeadingEdge2017,
  title = {The Role of the Leading Edge Vortex in Lift Augmentation of Steadily Revolving Wings: A Change in Perspective},
  shorttitle = {The Role of the Leading Edge Vortex in Lift Augmentation of Steadily Revolving Wings},
  author = {Nabawy, Mostafa R. A. and Crowther, William J.},
  year = 2017,
