@article{wuestAgilePerchingManeuvers2024,
  title = {Agile Perching Maneuvers in Birds and Morphing-Wing Drones},
  author = {W{\"u}est, Valentin and Jeger, Simon and Feroskhan, Mir and Ajanic, Enrico and Bergonti, Fabio and Floreano, Dario},
