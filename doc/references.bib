@article{tobalskeEffectsBodySize2000,
  title = {Effects of {{Body Size}} on {{Take-off Flight Performance}} in the {{Phasianidae}} ({{Aves}})},
  author = {Tobalske, Bret W. and Dial, Kenneth P.},
